{
    "manifest_version": 3,
    "name": "Time Out",
    "short_name": "Time Out",
    "version": "0.0.0.3",
    "default_locale": "en",
    "description": "track your browsering time ",
    "permissions": [
        "tabs",
        "activeTab",
        "alarms",
        "storage",
        "idle"
    ],
    "background": {
        "service_worker": "js/serviceWorker.js"
    },
    "minimum_chrome_version": "102",
    "action": {
        "default_title": "Time Out",
        "default_popup": "index.html",
        "default_icon": {
            "128": "images/icons/128.png"
        }
    },
    "icons": {
        "128": "images/icons/128.png"
    }
}